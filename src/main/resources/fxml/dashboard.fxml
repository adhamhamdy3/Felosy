<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="felosy.controllers.DashboardController">
   <children>
      <StackPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" prefHeight="86.0" prefWidth="357.0" text="Welcome, Dummy!" StackPane.alignment="TOP_CENTER">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="15.0" StackPane.alignment="BOTTOM_CENTER">
               <children>
                  <Label text="Main Menu">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Button fx:id="assetsInvestments_btn" mnemonicParsing="false" text="Assets and Investments ">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="zakatComp_btn" mnemonicParsing="false" prefHeight="26.0" prefWidth="143.0" text="Zakat and Compliance" />
                  <Button fx:id="reports_btn" mnemonicParsing="false" prefHeight="26.0" prefWidth="143.0" text="Reports" />
                  <Button fx:id="logout_btn" mnemonicParsing="false" text="Log out" textFill="RED">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <StackPane.margin>
                  <Insets top="70.0" />
               </StackPane.margin>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
